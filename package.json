{
  "name": "react-snake",
  "version": "0.1.0",
  "homepage": "https://ldgit.github.io/react-snake",
  "dependencies": {
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "seedrandom": "^3.0.5",
    "svelte": "^4.2.8",
    "uuid": "^9.0.1",
    "vite": "^5.0.11",
    "vite-tsconfig-paths": "^4.2.3",
    "web-vitals": "^3.5.1"
  },
  "scripts": {
    "start": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest",
    "test:all": "npm run lint:all && npm t -- run",
    "prettier": "prettier --check",
    "lint": "eslint --report-unused-disable-directives --ext .js,.jsx,.ts,.tsx",
    "lint:all": "npm run prettier -- . && npm run lint -- .",
    "prepare": "husky install"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react": "^14.1.2",
    "@typescript-eslint/eslint-plugin": "^6.18.0",
    "@typescript-eslint/parser": "^6.18.0",
    "autoprefixer": "^10.4.16",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-vitest": "^0.3.20",
    "husky": "^8.0.3",
    "jsdom": "^23.1.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8.4.33",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.3",
    "vitest": "^1.1.3",
    "vitest-dom": "^0.1.1"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "npm run lint -- --fix",
    "*.{js,jsx,ts,ts,tsx,css,md}": "npm run prettier -- --write"
  }
}
